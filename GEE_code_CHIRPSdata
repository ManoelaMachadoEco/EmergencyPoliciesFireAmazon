// Immediate regions from IBGE 
var ImmedRegAmaz = ImmedRegBR.filter(ee.Filter.bounds(AmazonBorder)); // 
print('ImmedRegAmaz:', ImmedRegAmaz);
Map.addLayer(ImmedRegAmaz, {color:'#85a86c'}, 'ImmedRegAmaz'); // 

